{
	"name": "mcu-e2e",
	"version": "1.0.58",
	"description": "mMCU E2E Automation",
	"main": "index.js",
	"scripts": {
		"test": "jest --verbose",
		"report": "node index.js",
		"build": "npm run pre-build; rm reports/*.html; npm run test",
		"builde2e": "npm run pre-build; rm reports/test-report.*; npm run test; npm run report",
		"srcformat": "npx prettier --write .",
		"lint": "eslint '{src,steps,common,test}/**/*.ts' --fix --no-error-on-unmatched-pattern",
		"pre-build": "npm run srcformat && npm run lint",
		"testc": "./node_modules/.bin/cucumber-js -p default",
		"format": "./node_modules/.bin/cucumber-js -f node_modules/cucumber-pretty",
		"load": "jest --verbose --config=jest.load.config.js",
		"rest": "jest --verbose --config=jest.rest.config.js",
		"manual": "jest --verbose --config=jest.manual.config.js",
		"rest-build": "rm reports/*.html; npm run rest; npm run report false reports/rest-sanity-report.json reports/rest-sanity-report.html",
		"unit-test": "jest --verbose --config=test/jest.unit.test.config.js",
		"runJobK8Dev": "curl http://10.107.153.7:8080/job/mcu-e2e-sanity/buildWithParameters?delay=10sec  --user jenkins:mavenir --data MCU_HOME=sipsvc-baremetal.il-labs.mavenir.com --data PLATFORM_TYPE=K8S",
		"build-common": "npm run pre-build; rm -rf dist; tsc; cp package*.json dist/;  cd dist; npm pack"
	},
